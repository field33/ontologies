  rdfs:label "Part Of"@en ;
  rdfs:range :19993 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCActivityPartOfAPQCProcess> .

:10091 rdf:type owl:Class ;
  rdfs:comment "Identifying performance parameters. Determine the parameters to measure performance of the design and development of the product/service concepts into prototypes. "@en ;
  rdfs:label "Identify design/development performance indicators"@en ;
  rdfs:subClassOf <http://field33.com/ontologies/@apqc/apqc/APQCActivity> .

:10091FollowedBy10092 rdf:type owl:ObjectProperty ;

  rdfs:range :10129 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCActivityPartOfAPQCProcess> .

:10137 rdf:type owl:Class ;
  rdfs:comment "Reviewing promotional activities' effect on the sales orders. Analyze all promotional events and campaigns that the organization has already employed or is planning to deploy. "@en ;
  rdfs:label "Analyze historical and planned promotions and events"@en ;
  rdfs:subClassOf <http://field33.com/ontologies/@apqc/apqc/APQCActivity> .


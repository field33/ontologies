  rdfs:domain :10188 ;
  rdfs:label "Part Of"@en ;
  rdfs:range :10182 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCActivityPartOfAPQCProcess> .

:10189 rdf:type owl:Class ;
  rdfs:comment "Qualifying the prospective customers into credible leads by gauging their behavior against the organization's offering. Triangulate leads to increase the efficiency of sales and marketing efforts. Build a detailed profile of the prospects. Determine what products/services they already use, if they have decision-making authority, their views on the products/services they already use, how prone they are to switch, if the organization's solution better in some attributes than those prospects currently use, etc."@en ;
  rdfs:label "Identify/receive leads/opportunities"@en ;
  rdfs:subClassOf <http://field33.com/ontologies/@apqc/apqc/APQCActivity> .

:10189FollowedBy18115 rdf:type owl:ObjectProperty ;
  rdfs:domain :10189 ;
  rdfs:label "Followed By"@en ;
  rdfs:range :18115 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCActivityFollowedByAPQCActivity> .

:10189PartOf10182 rdf:type owl:ObjectProperty ;
  rdfs:domain :10189 ;
  rdfs:label "Part Of"@en ;
  rdfs:range :10182 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCActivityPartOfAPQCProcess> .

:10190 rdf:type owl:Class ;
  rdfs:comment "Communicating with customers and prospects with the intent of creating sales opportunities. Reach out to existing and prospective customers through alternate media and networking channels, apart from cold calling/emailing."@en ;
  rdfs:label "Perform sales calls"@en ;
  rdfs:subClassOf <http://field33.com/ontologies/@apqc/apqc/APQCTask> .

:10190FollowedBy10191 rdf:type owl:ObjectProperty ;
  rdfs:domain :10190 ;
  rdfs:label "Followed By"@en ;
  rdfs:range :10191 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCTaskFollowedByAPQCTask> .


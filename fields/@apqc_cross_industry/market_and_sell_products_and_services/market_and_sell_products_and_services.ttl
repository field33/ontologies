  rdfs:range :10177 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCActivityFollowedByAPQCActivity> .

:10176PartOf10153 rdf:type owl:ObjectProperty ;
  rdfs:domain :10176 ;
  rdfs:label "Part Of"@en ;
  rdfs:range :10153 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCActivityPartOfAPQCProcess> .

:10177 rdf:type owl:Class ;
  rdfs:comment "Reviewing and reappraising the strategies, objectives, and plans for all customer-centered processes. Revisit all customer-focused processes and activities--which relate to their acquisition, conversion, and retention--with the objective of revising them in light of customer analysis. Revise accordingly."@en ;
  rdfs:label "Revise customer strategies, objectives, and plans based on metrics"@en ;
  rdfs:subClassOf <http://field33.com/ontologies/@apqc/apqc/APQCActivity> .


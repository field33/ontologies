  rdfs:label "Part Of"@en ;
  rdfs:range :10103 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCProcessPartOfAPQCProcessGroup> .

:10130 rdf:type owl:Class ;
  rdfs:comment "Cultivating an alliance of partners by identifying, analyzing, negotiating, and managing partnerships with other economic agents."@en ;
  rdfs:label "Develop sales partner/alliance relationships"@en ;
  rdfs:subClassOf <http://field33.com/ontologies/@apqc/apqc/APQCProcess> .


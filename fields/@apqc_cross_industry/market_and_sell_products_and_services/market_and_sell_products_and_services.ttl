  rdfs:label "Part Of"@en ;
  rdfs:range :10149 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCActivityPartOfAPQCProcess> .

:10157 rdf:type owl:Class ;
  rdfs:comment "Estimating the outlay required for promoting, selling, and distributing the products/services of the organization. Add up the expenses of all activities necessitated in marketing, such as promotional campaigns, advertising, marketing communications, PR campaigns, employing skilled personnel, and office space."@en ;
  rdfs:label "Create marketing budget"@en ;
  rdfs:subClassOf <http://field33.com/ontologies/@apqc/apqc/APQCActivity> .


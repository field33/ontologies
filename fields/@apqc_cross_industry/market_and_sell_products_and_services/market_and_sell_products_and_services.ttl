  rdfs:label "Part Of"@en ;
  rdfs:range :10103 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCProcessPartOfAPQCProcessGroup> .

:10134 rdf:type owl:Class ;
  rdfs:comment "Gathering all information about sales orders into an index. Create a directory of all sales orders, whether open or those which have been fulfilled. Track what product/service was ordered, the quantity ordered, who ordered it, the delivery date, the shipping method, the unit price and line total, payment terms, and any discount applied."@en ;
  rdfs:label "Gather current and historic order information"@en ;
  rdfs:subClassOf <http://field33.com/ontologies/@apqc/apqc/APQCActivity> .

:10134FollowedBy10135 rdf:type owl:ObjectProperty ;
  rdfs:domain :10134 ;
  rdfs:label "Followed By"@en ;
  rdfs:range :10135 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCActivityFollowedByAPQCActivity> .


  rdfs:range :10154 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCActivityPartOfAPQCProcess> .

:10179 rdf:type owl:Class ;
  rdfs:comment "Piloting the packaged products/services in the market with a test audience. Create trial runs using techniques such as focus groups of the final product, wrapped and bundled."@en ;
  rdfs:label "Test packaging options"@en ;
  rdfs:subClassOf <http://field33.com/ontologies/@apqc/apqc/APQCActivity> .

:10179FollowedBy10180 rdf:type owl:ObjectProperty ;
  rdfs:domain :10179 ;
  rdfs:label "Followed By"@en ;
  rdfs:range :10180 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCActivityFollowedByAPQCActivity> .

:10179PartOf10154 rdf:type owl:ObjectProperty ;

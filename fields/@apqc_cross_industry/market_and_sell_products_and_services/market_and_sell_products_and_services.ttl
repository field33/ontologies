  rdfs:label "Part Of"@en ;
  rdfs:range :10184 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCTaskPartOfAPQCActivity> .

:10191 rdf:type owl:Class ;

  rdfs:range :10149 ;
  rdfs:subPropertyOf <http://field33.com/ontologies/@apqc/apqc/APQCActivityPartOfAPQCProcess> .

:10159 rdf:type owl:Class ;
  rdfs:comment "Developing the central messages for a segment of its customers. Craft concise statements that position the value proposition of individual products/services around the pressing concerns in the market, thereby showing how the organization's offerings are the right fit for the customers."@en ;
  rdfs:label "Develop marketing messages"@en ;
  rdfs:subClassOf <http://field33.com/ontologies/@apqc/apqc/APQCActivity> .

:10159FollowedBy10160 rdf:type owl:ObjectProperty ;
